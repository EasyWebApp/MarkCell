{
    "compilerOptions": {
        "outDir": "dist/",
        "sourceMap": true,
        "declaration": true,
        "target": "ES6",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "jsx": "react",
        "jsxFactory": "createCell"
    },
    "include": ["source/**/*"],
    "typedocOptions": {
        "name": "MarkCell",
        "mode": "file",
        "excludeExternals": true,
        "excludeNotExported": true,
        "readme": "./ReadMe.md",
        "out": "docs/"
    }
}
